{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch API on Port 5000",
        "program": "${workspaceFolder}/src/index.js",
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "development",
          "PORT": "5000"
        },
        "console": "integratedTerminal",
        "restart": true
      }
    ]
  }
  